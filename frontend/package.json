    "@mui/lab": "^5.0.0-alpha.171",
    "@mui/material": "^5.16.0",
    "@mui/x-date-pickers": "^7.9.0",
    "@tanstack/react-query": "^5.51.1",
    "axios": "^1.7.2",
    "build": "^0.1.4",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "formik": "^2.4.6",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
