{
  "name": "homeservice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "yarn": "yarn install",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "convert-jsx-to-tsx": "find src -type f -name '*.jsx' -exec sh -c 'mv \"$1\" \"${1%.jsx}.tsx\"' - {} \\;",
    "convert-js-to-ts": "find src -type f -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.ts\"' - {} \\;"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@material/button": "^14.0.0",
