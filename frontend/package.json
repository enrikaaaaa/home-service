    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "react-day-picker": "^8.10.1",
    "react-responsive": "^10.0.0",
    "typescript": "^5.5.2",
    "vite": "^5.1.6",
    "ts-node": "^10.9.2"
